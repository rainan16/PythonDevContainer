{
    "cSpell.words": [
        "devcontainer",
        "devcontainers"
    ],
    "cSpell.ignorePaths": [
        "requirements.txt",
        ".devcontainer",
        ".git/objects",
        ".github/workflows",
        ".vscode",
        ".vscode-insiders"
    ],
    "flake8.args": [
        "--max-line-length=120"
    ],    
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    }
}
